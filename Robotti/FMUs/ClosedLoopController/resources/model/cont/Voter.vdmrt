class Voter
    
    operations

    public Voter: () ==> Voter
    Voter() == (
        skip;
    );

    -- Do TMR on the three inputs
    public choose_value: real * real * real ==> real
    choose_value(v1,v2,v3) == (
        if (v1 = v2)
        then (return v1;)
        else (
            if (v1 = v3)
            then (return v1;)
            else (
                if (v2 = v3)
                then (return v2;)
                else (error);
            );
        );
    );

end Voter